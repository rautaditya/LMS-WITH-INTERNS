<div class="section" id="allcourses">
  <div><h1>this is student page our courses</h1></div>
  <% courses.forEach(course => { %>
      <div class="containerforcourse">
        <img class="" src="/uploads/<%= course.courseImage %>" alt="<%= course.courseName %> Image">
        <h2><%= course.courseName %></h2>
        <p><span>Professor</span>: <%= course.courseProfessorName %></p>
        <p><span>Course Price</span>: <%= course.courseDescription %></p>
        <div class="coursebtns">
          <a href="/enroll/<%= course._id %>">
            <button class="enrollbtn">Enroll</button>
        </a>          <!-- Add more details as needed -->
        <a href="/courseoverview/<%= course._id %>">
          <button class="enrollbtn">Course Overview</button>
      </a>
        </div>
      </div>
    <% }); %>
</div>



